generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int      @id @default(autoincrement())
  event_title String   @unique
  date        DateTime
  description String
  audio_url   String?
  video_url   String?
  handout_url String?
}
